  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/eluv-io/elv-embed",
  "scripts": {
    "bump-version": "npm --git-tag-version --no-commit-hooks version patch",
    "serve": "webpack-dev-server --hot --port 8088 --host=0.0.0.0",
    "build": "node ./node_modules/webpack-cli/bin/cli.js --mode=production --devtool false",
    "build-analyze": "ANALYZE_BUNDLE=true node ./node_modules/webpack-cli/bin/cli.js --mode=production --devtool false",
    "add-build": "COMMIT=`git rev-parse HEAD` ; echo $COMMIT > dist/version.txt ; git add dist",
    "lint": "npm run fix-scss ; npm run lint-scss ; npm run fix-js",
    "lint-show": "npm run lint-scss ; npm run lint-js",
    "lint-js": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "lint-scss": "./node_modules/sass-lint/bin/sass-lint.js -c .scss-lint.yml -v -q",
    "fix-js": "npm run lint-js -- --fix",
    "fix-scss": "./node_modules/sass-lint-auto-fix/dist/index.js"
  },
  "dependencies": {
    "@eluvio/elv-client-js": "^4.0.126",
    "@eluvio/elv-player-js": "2.0.44",
    "@mantine/core": "~7.4.0",
    "@mantine/form": "~7.4.0",
    "epubjs": "^0.3.92",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-inlinesvg": "^4.1.0",
    "scroll-behavior-polyfill": "^2.0.13",
    "swiper": "^8.3.2",
    "ua-parser-js": "^2.0.3",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/runtime": "^7.23.1",
    "autoprefixer": "^10.4.16",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.5.3",
    "postcss-loader": "^7.3.3",
    "regenerator-runtime": "^0.14.0",
    "sass": "^1.69.2",
    "sass-lint": "^1.12.1",
    "sass-lint-auto-fix": "^0.21.2",
    "sass-loader": "^13.3.2",
    "source-map-loader": "^0.2.4",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.3",
    "svg-inline-loader": "^0.8.2",
    "url": "^0.11.3",
    "uuid": "^7.0.2",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
